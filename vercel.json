{
  "version": 2,
  "builds": [
    {
      "src": "/build_files.sh",
      "use": "@ardnt/vercel-python-wsgi",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "/ipx/wsgi.py",
      "use": "@vercel/static-build",
      "config": { "distDir": "staticfiles" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/ipx/wsgi.py"
    }
  ]
}