{
  "version": 2,
  "builds": [
    {
      "src": "ipx/wsgi.py",
      "use": "@ardnt/vercel-python-wsgi",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "/build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "ui/staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "ipx/wsgi.py"
    }
  ],
  "outputDirectory": "ui/staticfiles"
}
